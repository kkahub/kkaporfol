{
  "name": "kka-porfol",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "homepage": "https://kkahub.github.io/kkaporfol/",
  "scripts": {
    "dev": "next dev",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "sitemap": "next-sitemap  --config next-sitemap.config.js",
    "predeploy": "next build && next-sitemap && type NUL > out/.nojekyll && git add .",
    "deploy": "gh-pages -d out --dotfiles",
    "json": "json-server --watch src/data/portfolio.json --port 3001",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "dependencies": {
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.14",
    "@mui/styled-engine-sc": "^6.0.0-alpha.2",
    "@next/bundle-analyzer": "16.0.8",
    "@reduxjs/toolkit": "^1.9.6",
    "@types/d3": "^7.4.3",
    "@types/lodash": "^4.14.202",
    "@types/node": "22.13.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/redux-logger": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "axios": "^1.6.2",
    "d3": "^7.8.5",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.8",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "framer-motion": "^12.23.25",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.3",
    "json-server": "^1.0.0-alpha.23",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "next": "16.0.8",
    "next-redux-wrapper": "^8.1.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.2",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.3",
    "sass": "^1.64.2",
    "sass-migrator": "^2.4.2",
    "sharp": "^0.32.6",
    "styled-components": "^6.1.1",
    "swiper": "^10.2.0",
    "typescript": "5",
    "typescript-plugin-css-modules": "^5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, jsx,ts,tsx}": [
      "eslint . --fix",
      "prettier --write"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@types/gtag.js": "^0.0.17",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.0.3"
  },
  "overrides": {
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  }
}
