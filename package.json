{
  "name": "kka-porfol",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "homepage": "https://kkahub.github.io/kkaporfol/",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prepare": "husky install",
    "predeploy": "next build && next-sitemap && next export && type NUL > out/.nojekyll && git add .",
    "deploy": "gh-pages -d out --dotfiles",
    "sitemap": "next-sitemap  --config next-sitemap.config.js"
  },
  "dependencies": {
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.14",
    "@mui/styled-engine-sc": "^6.0.0-alpha.2",
    "@reduxjs/toolkit": "^1.9.6",
    "@types/d3": "^7.4.3",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/redux-logger": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "5",
    "@typescript-eslint/parser": "5",
    "axios": "^1.6.2",
    "d3": "^7.8.5",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^14.0.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "framer-motion": "^10.16.4",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "next": "^14.0.2",
    "next-redux-wrapper": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.2",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.3",
    "sass": "^1.64.2",
    "sharp": "^0.32.6",
    "swiper": "^10.2.0",
    "typescript": "5",
    "typescript-plugin-css-modules": "^5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/gtag.js": "^0.0.17",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.0.3"
  }
}
